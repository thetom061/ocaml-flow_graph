digraph finite_state_machine {
	rankdir=LR;
	size="8,5"
	node [shape=circle]; 2 5 1 4 -1 3 6 0 ;
2 -> 4 [label = "0"];
2 -> 5 [label = "1"];
5 -> 0 [label = "1"];
1 -> 5 [label = "0"];
1 -> 4 [label = "1"];
4 -> 0 [label = "1"];
-1 -> 2 [label = "1"];
-1 -> 1 [label = "1"];
-1 -> 3 [label = "1"];
3 -> 5 [label = "0"];
3 -> 6 [label = "1"];
6 -> 0 [label = "1"];
}